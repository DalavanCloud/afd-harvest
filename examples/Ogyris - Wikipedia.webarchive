										isParserVar = true;
										wikEd.HighlightBuildTree('templateParserFunct', 'block', tagFrom + 2, innerPlain.length, parseObj);
									}
								}
							}

							// {{VARIABLE:R}}
							if (isParserVar === false) {
								if ( (ns !== '') && (template == 'R') ) {
