							param = regExpMatch[8] || '';
							var parserVar = ns.substr(0, ns.length - 1);

							// {{VARIABLE}}
							if (isParserVar === false) {
								if ( (template !== '') && (ns === '') && (param === '') ) {
									var regExpParserVar = new RegExp('^(' + wikEd.parserVariables + '|' + wikEd.parserVariablesR + ')$', '');
