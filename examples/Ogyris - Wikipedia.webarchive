											var item = '';
											var regExpMatchItem;
											if ( (regExpMatchItem = /(^|\|)id=([^|]+)/.exec(param)) !== null) {
												item = regExpMatchItem[2];
											}

											// item name from parameter
											else if ( (regExpMatchItem = /(^|\|)of=([^|]+)/.exec(param)) !== null) {
