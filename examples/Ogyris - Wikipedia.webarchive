									var regExpMatchParserVar;
									if ( (regExpMatchParserVar = regExpParserVar.exec(parserVar)) !== null) {
										isParserVar = true;
										wikEd.HighlightBuildTree('templateParserFunct', 'block', tagFrom + 2, innerPlain.indexOf(':') + 1, parseObj);
									}
								}
							}

							// {{FUNCTION:param|R}}
							if (isParserVar === false) {
								if ( (ns !== '') && ( (param === '') || (param == 'R') ) ) {
