										wikEd.HighlightBuildTree('templateParserFunct', 'block', tagFrom + 2, innerPlain.indexOf(':') + 1, parseObj);
									}
								}
							}

							// {{function:param|param}}
							if (isParserVar === false) {
								if (ns !== '') {
